{
    "manifest_version": 3,
    "name": "Multi Search",
    "version": "1.0",
    "description": "Search multiple engines (Google, Bing, DuckDuckGo, Yahoo, Ecosia, Qwant) at once.",
    "permissions": ["tabs"],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  